  <script>

    searchList()
    function searchList() {
      $.ajax({
        url: 'http://localhost:3000/trainee/getMainData',
        type: 'get',
        success: function (data) {
          console.log(data);       
          
          for (var i = 0; i < data.length; i++) {
            if (i % 4 == 0) $("#mainList").append('<br>');
            $("#mainList").append('<a href =' + 'http://localhost:3000/traineeInfo?id=' + data[i].trainee_idx + '>' + '<img src="' + data[i].photo1 + '" vspace=10 hspace=10>');
          }
        }
    })
  }



    $('#logoutBtn').click(function () {
      var jwtToken = sessionStorage.getItem('jwtToken');
      //console.log(jwtToken);
      sessionStorage.removeItem('jwtToken');
      alert('로그아웃 되었습니다');
      console.log('로그아웃');
      location.replace('/');
    })
    $('#mypageBtn').click(function () {
      var jwtToken = sessionStorage.getItem('jwtToken');
      if (jwtToken == null) {
        alert('로그인해주세요');
        location.replace('/login');
      } else {
        location.replace('/mypage')
      }
    })

